  <link rel="stylesheet" href="/css/admin/programs.css" />

  <div class="content">

    <table id="monitors">
      <thead>
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Program</th>
        </tr>
      </thead>
      <tbody>
        {{#each monitors}}
        <tr>
          <td>{{this.UserID}}</td>
          <td>{{this.Username}}</td>
          <td>
            {{this.ProgramID}}
          </td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>


  <script defer>
    new DataTable('#monitors');
  </script>